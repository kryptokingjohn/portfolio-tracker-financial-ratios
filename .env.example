# Portfolio Tracker Environment Variables
# Copy this file to .env.local and fill in your actual values
# NEVER commit .env.local or any file with real API keys to version control

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Financial Modeling Prep API
VITE_FMP_API_KEY=your_fmp_api_key

# Stripe Configuration (Publishable key only - Secret key goes on server)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_test_key_or_pk_live_your_live_key

# Development/Demo Mode
VITE_DEMO_MODE=false

# Security Configuration
VITE_ENABLE_AUDIT_LOGGING=true
VITE_SESSION_TIMEOUT_MINUTES=30

# Session Management
# Maximum session duration in minutes (default: 30)
# Inactivity timeout is fixed at 15 minutes for security