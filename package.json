{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy:prepare": "node scripts/deploy.js",
    "deploy:netlify": "npm run deploy:prepare && echo 'Build ready! Upload dist folder to netlify.com/drop'",
    "deploy:netlify-cli": "npm run deploy:prepare && netlify deploy --prod --dir=dist",
    "deploy:vercel": "npm run deploy:prepare && vercel --prod",
    "build:android": "npm run build && npx cap sync android && npx cap open android",
    "android:prepare": "node scripts/android-build.js",
    "android:open": "npx cap open android",
    "android": "npx cap run android",
    "sync": "npx cap sync",
    "populate:sp500": "node scripts/populate-sp500-companies.js",
    "populate:sp500-simple": "node scripts/populate-sp500-simple.js",
    "populate:companies": "node scripts/populate-companies-basic.js",
    "add-missing:sp500": "node scripts/add-missing-sp500.js",
    "secure:database": "node scripts/re-enable-rls.js",
    "setup:subscriptions": "node scripts/setup-subscriptions-table.js",
    "check:subscriptions": "node scripts/check-subscription-columns.js",
    "fix:subscription-rls": "node scripts/fix-subscription-rls.js",
    "test:table-structure": "node scripts/test-table-structure.js",
    "test:etf-api": "node scripts/test-etf-api.js"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.2",
    "@capacitor/app": "^7.0.1",
    "@capacitor/cli": "^7.4.2",
    "@capacitor/core": "^7.4.2",
    "@capacitor/preferences": "^7.0.1",
    "@capacitor/splash-screen": "^7.0.1",
    "@capacitor/status-bar": "^7.0.1",
    "@stripe/react-stripe-js": "^3.8.1",
    "@stripe/stripe-js": "^7.7.0",
    "@supabase/supabase-js": "^2.52.0",
    "@types/node": "^24.0.15",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "stripe": "^14.25.0"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "sharp": "^0.34.3",
    "svg2png-wasm": "^1.4.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vite-plugin-pwa": "^1.0.1",
    "workbox-window": "^7.3.0"
  }
}
